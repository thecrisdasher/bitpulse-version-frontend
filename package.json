{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3004",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force -Path .next -ErrorAction SilentlyContinue; Remove-Item -Recurse -Force -Path node_modules/.cache -ErrorAction SilentlyContinue\"",
    "clean:start": "npm run clean && npm run dev",
    "kill-port": "powershell -Command \"$processes = Get-Process -Id (Get-NetTCPConnection -LocalPort 3004 -ErrorAction SilentlyContinue).OwningProcess -ErrorAction SilentlyContinue; if ($processes) { $processes | ForEach-Object { Stop-Process -Id $_.Id -Force -ErrorAction SilentlyContinue }; Write-Output 'Procesos terminados' } else { Write-Output 'No hay procesos usando el puerto 3004' }\"",
    "reset": "npm run kill-port && npm run clean && npm run dev"
  },
